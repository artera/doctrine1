includes:
  - phpstan-baseline.neon
  - phpstan-review.neon

parameters:
  level: 8
  tipsOfTheDay: false
  phpVersion: 80000
  checkMissingIterableValueType: false
  checkGenericClassInNonGenericObjectType: false
  inferPrivatePropertyTypeFromConstructor: true
  parallel:
    processTimeout: 300.0
  paths:
    - lib
